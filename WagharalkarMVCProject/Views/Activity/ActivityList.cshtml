<main class="">
    <section id="why-us">
        <div class="container">

            <div class="row">
                <div class="col-lg-12 d-flex align-items-stretch">
                    <div class="table-responsive">

                        <table id="tblactivity" class="table table-bordered table-light">
                            <thead class="table-dark">
                                <tr class="table-danger">
