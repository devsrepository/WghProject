    </section>
    <section id="why-us">
        <div id="CategoryModal" class="modal fade" role="dialog">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-body">
                        <div class="container" id="DetailCategory">
                            <table id="tblactivity" class="table table-bordered table-dark table-hover table-sm">
                                <thead class="table-responsive">
                                    <tr class="table-hover">
                                        <th class="table-active">ID</th>
                                        <th class="table-primary">Title</th>
                                        <th class="table-secondary">Details</th>
                                        <th class="table-danger">Image1</th>
                                        <th class="table-dark">Image2</th>
                                        <th class="table-warning">Type</th>
                                        <th class="table-info">Date</th>
                                        <th class="table-secondary">CreateDate</th>
                                        <th class="table-success">UpdateDate</th>
                                        <th class="table-light">Createdby</th>
                                        <th class="table-danger">Updatedby</th>
                                    </tr>

                                </thead>
                                <tbody>
                                </tbody>

                            </table>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">CLOSE</button>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </section>
</main>
